      <Minimum>-9999</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="s16 貢献度予備2">
      <Minimum>-9999</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="u16 出現貢献度タイプ = 9999">
      <Enum>CP_TYPE</Enum>
      <Description>貢献度が出現条件のミッションのみ設定</Description>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="s16 必要貢献度">
      <Minimum>-9999</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="u8 ムービーID">
      <Description>ミッションクリア後に再生するミッションのみ設定</Description>
    </Field>
    <Field Def="u8 ミッション出現条件 Sアリーナランク" />
    <Field Def="u8 共同ミッション選択可能フラグ">
      <Enum>ON_OFF</Enum>
      <Description>ONで選択可能に</Description>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="u8 2週目ムービーID">
      <Description>2週目以降再生</Description>
    </Field>
    <Field Def="u16 AC対戦時メッセージシステムのID">
      <Description>AC対戦時メッセージシステムを使用かつ僚機がいないときにメッセージを再生したいときに設定</Description>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="u16 依頼者">
      <Enum>REQUESTER_TYPE</Enum>
      <EditFlags>Lock</EditFlags>
    </Field>
