    <Field Def="f32 重力加速度">
      <Minimum>0</Minimum>
      <Maximum>999</Maximum>
    </Field>
    <Field Def="s32 加速開始フレーム">
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="s32 加速終了フレーム">
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="s32 通常アクション開始フレーム">
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="s32 通常アクション終了フレーム">
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
    </Field>
