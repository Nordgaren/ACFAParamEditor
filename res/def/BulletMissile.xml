      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.05</Increment>
    </Field>
    <Field Def="f32 減衰：貫通力">
      <Minimum>0</Minimum>
      <Maximum>1</Maximum>
      <Increment>0.05</Increment>
    </Field>
