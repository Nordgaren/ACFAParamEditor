      <Description>接近アクションを開始する（ターゲットとの）距離</Description>
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
      <Increment>10</Increment>
    </Field>
    <Field Def="u8 接近アクション">
      <Enum>MISSILE_ACTION_ID</Enum>
      <EditFlags>None</EditFlags>
      <Maximum>99</Maximum>
    </Field>
