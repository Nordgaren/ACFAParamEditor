      <Enum>MISSILE_ACTION_ID</Enum>
      <Description>加速開始後に行うアクション</Description>
      <EditFlags>None</EditFlags>
      <Maximum>99</Maximum>
    </Field>
    <Field Def="dummy8 --予約--[3]">
      <DisplayFormat>%d</DisplayFormat>
      <Maximum>1</Maximum>
      <Increment>1</Increment>
    </Field>
