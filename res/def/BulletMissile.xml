      <Description>初期高度からこの高度まで落下するとアクション開始</Description>
      <Minimum>-9999</Minimum>
      <Maximum>9999</Maximum>
      <Increment>1</Increment>
    </Field>
    <Field Def="u8 高度指定アクション">
      <Enum>MISSILE_ACTION_ID</Enum>
      <EditFlags>None</EditFlags>
      <Maximum>99</Maximum>
    </Field>
    <Field Def="u8 ターゲットロスト後アクション">
      <Enum>MISSILE_ACTION_ID</Enum>
      <EditFlags>None</EditFlags>
      <Maximum>99</Maximum>
    </Field>
    <Field Def="u8 デコイ耐性 = 100">
      <EditFlags>None</EditFlags>
      <Minimum>1</Minimum>
    </Field>
    <Field Def="u8 群制御タイプ">
      <Enum>MISSILE_CROWD_ID</Enum>
      <EditFlags>None</EditFlags>
      <Maximum>99</Maximum>
    </Field>
    <Field Def="u8 群制御：最小広がり幅（半径）" />
    <Field Def="u8 群制御：最大広がり幅（半径）" />
