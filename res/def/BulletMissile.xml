      <Enum>BULLET_BEHAVIOR_ID</Enum>
      <EditFlags>Wrap, Lock</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>9999</Maximum>
    </Field>
    <Field Def="u8 撃墜可">
      <Enum>ON_OFF</Enum>
      <Description>この弾が「迎撃可」の弾によって撃ち落されることができる</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 水中可">
      <Enum>ON_OFF</Enum>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 金網貫通">
      <Enum>ON_OFF</Enum>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="u8 迎撃可 = 1">
      <Enum>ON_OFF</Enum>
      <Description>この弾が「撃墜可」の弾を撃ち落すことができる</Description>
      <EditFlags>None</EditFlags>
      <Maximum>1</Maximum>
    </Field>
    <Field Def="f32 当たり半径">
      <Description>0 なら直線当たり</Description>
      <DisplayFormat>%.1f</DisplayFormat>
      <EditFlags>None</EditFlags>
      <Minimum>0</Minimum>
      <Maximum>99</Maximum>
      <Increment>0.1</Increment>
    </Field>
